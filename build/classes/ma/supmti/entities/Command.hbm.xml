        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property generated="never" lazy="false" name="label" optimistic-lock="true" type="string" unique="false">
            <column name="label" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="prix" optimistic-lock="true" type="java.lang.Double" unique="false">
            <column name="prix" precision="22" scale="0"/>
        </property>
        <property generated="never" lazy="false" name="date_command" optimistic-lock="true" type="string" unique="false">
            <column  name="date_command"/>
        </property>
       
    </class>
</hibernate-mapping>
